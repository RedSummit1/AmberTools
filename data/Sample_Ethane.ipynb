    "import py3Dmol\n",
    "\n",
    "# Load and visualize ethane\n",
    "view = py3Dmol.view(width=800, height=600)\n",
    "view.addModel(open('ethane_final.pdb').read(), 'pdb')\n",
    "view.setStyle({'stick': {'radius': 0.2}, 'sphere': {'radius': 0.4}})\n",
    "view.setBackgroundColor('white')\n",
    "view.zoomTo()\n",
    "view.show()"
   ]
